{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",       
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "rules": {
        "no-unused-vars": [1, { "args": "after-used", "argsIgnorePattern": "^_" }]
    }
}
